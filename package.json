{
  "name": "beautiful-react-hooks",
  "version": "0.14.0",
  "description": "A collection of beautiful (and hopefully useful) React hooks to speed-up your components and hooks development",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --ext .js src/",
    "build-js": "NODE_ENV=production gulp -f ./scripts/build.js",
    "build": "npm run build-js && npm run docs",
    "test": "NODE_ENV=test nyc mocha --exit",
    "test:watch": "npm run test -- --watch",
    "start": "npx styleguidist server --config docs/styleguidist/styleguidist.config.js --open",
    "docs": "npx styleguidist build --config docs/styleguidist/styleguidist.config.js"
  },
  "files": [
    "dist/**",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonioru/beautiful-react-hooks.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/antonioru/beautiful-react-hooks/issues"
  },
  "homepage": "https://github.com/antonioru/beautiful-react-hooks#readme",
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.7",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "chai": "^4.2.0",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "jsdoc-to-markdown": "^5.0.3",
